{
  "name": "@openreachtech/eslint-inspector",
  "version": "2.0.1",
  "description": "This is the useful tools for for JavaScript test concerning ESLint configuration.",
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
