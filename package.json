  "bugs": {
    "url": "https://github.com/openreachtech/eslint-inspector/issues"
  },
  "homepage": "https://github.com/openreachtech/eslint-inspector#readme",
  "devDependencies": {
    "@openreachtech/eslint-config": "^3.0.1",
    "@openreachtech/renchan-test-tools": "^1.0.0",
    "@types/jest": "^29.5.12",
    "eslint": "^9.3.0",
    "jest": "^29.7.0"
  }
